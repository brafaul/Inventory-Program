<Window x:Class="Management_Program.RemoveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Management_Program"
        mc:Ignorable="d"
        Title="RemoveWindow" Height="300" Width="300">
    <Viewbox>
        <Grid>
            <TextBox x:Name="RemNameBox" Background="LightGray" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="RemNameBox_TextChanged"/>
            <Label x:Name="NameLabel" Content="Remove By Name" Background="LightGray" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" Width="120"/>
            <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Center" Margin="0,239,75,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
            <Button x:Name="ConfirmButton" Content="Confirm" HorizontalAlignment="Left" Margin="151,239,0,0" VerticalAlignment="Top" Width="75" Click="ConfirmButton_Click"/>
        </Grid>
    </Viewbox>
</Window>
